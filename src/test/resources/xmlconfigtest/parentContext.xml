<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="myCustomer" class="zhy2002.springexamples.domain.Customer">
        <property name="lastName" value="Customer In Parent Context" />
    </bean>

    <bean id="newCustomer" class="zhy2002.springexamples.domain.Customer">
        <property name="lastName" value="New Customer In Parent Context" />
    </bean>
</beans>