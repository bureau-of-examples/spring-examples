<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="lazy1" class="zhy2002.springexamples.common.LazyTestObject" lazy-init="true" />

    <bean id="lazy2" class="zhy2002.springexamples.common.LazyTestObject" lazy-init="true" />

    <bean id="notLazy" class="zhy2002.springexamples.common.LazyTestObject" depends-on="lazy1" />

</beans>